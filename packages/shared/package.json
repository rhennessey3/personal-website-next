    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@personal-website/config": "*",
    "@types/node": "^18.17.0",
    "eslint": "^8.52.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "zod": "^3.22.2"
  }
}